<h1>Log In</h1>
<%= form_tag login_path do %>
  <%= label_tag :username%><br>
  <%= text_field_tag :username %><br>
  <%= label_tag :password %><br>
  <%= password_field_tag :password %><br><br>
  <%= submit_tag "Log In" %>
<% end %><br>

<p><%= link_to "Login in Via GitHub", "auth/github"%></p>

 <br>New User? Please <%= link_to "Sign Up", signup_path %><br>
