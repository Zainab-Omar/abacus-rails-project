<% if @expense.errors.any? %>
  <div id="error_explanation">
    <ul>
    <% @expense.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>
<%= form_for @group do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br><br>

  <%= f.submit %><br>
<% end %>
